{
  "kind": "collectionType",
  "collectionName": "deployStatuses",
  "info": {
    "singularName": "deploy-status",
    "pluralName": "deploy-statuses",
    "displayName": "deployStatus"
  },
  "options": {
    "draftAndPublish": false,
    "comment": ""
  },
  "attributes": {
    "message": {
      "type": "text"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "info",
        "warning",
        "error"
      ]
    },
    "stage": {
      "type": "string"
    },
    "deploy": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::make-deploy.deploy",
      "inversedBy": "deployStatus"
    }
  }
}
