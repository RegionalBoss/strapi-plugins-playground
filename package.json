{
  "name": "strapi-plugins-playground",
  "private": true,
  "version": "0.1.0",
  "description": "A Strapi application",
  "scripts": {
    "dev": "yarn build && yarn develop --watch-admin",
    "develop": "strapi develop",
    "start": "strapi start",
    "build": "strapi build",
    "prebuild": "yarn build:cloudinary && yarn build:deploy && yarn build:hierarchy",
    "build:cloudinary": "cd ./src/plugins/cloudinary-field && yarn build && cd ../../..",
    "build:deploy": "cd ./src/plugins/make-deploy && yarn build && cd ../../..",
    "build:hierarchy": "cd ./src/plugins/page-hierarchy && yarn build && cd ../../..",
    "strapi": "strapi",
    "postinstall": "yarn run install:cloudinary && yarn run install:deploy && yarn run install:hierarchy",
    "install:cloudinary": "cd ./src/plugins/cloudinary-field && yarn && cd ../../..",
    "install:deploy": "cd ./src/plugins/make-deploy && yarn && cd ../../..",
    "install:hierarchy": "cd ./src/plugins/page-hierarchy && yarn && cd ../../.."
  },
  "dependencies": {
    "@strapi/plugin-graphql": "4.8.2",
    "@strapi/plugin-i18n": "4.8.2",
    "@strapi/plugin-users-permissions": "4.8.2",
    "@strapi/strapi": "4.8.2",
    "better-sqlite3": "7.6.2",
    "pg": "^8.8.0",
    "strapi-plugin-comments": "^2.2.1"
  },
  "author": {
    "name": "A Strapi developer"
  },
  "strapi": {
    "uuid": "c3456380-1507-4b9e-9d37-b293c2335bd5"
  },
  "engines": {
    "node": ">=12.x.x <=18.x.x",
    "npm": ">=6.0.0"
  },
  "license": "MIT"
}
